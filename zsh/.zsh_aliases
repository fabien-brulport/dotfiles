alias l="ls -alth"

# Alias coming from the oh-my-zsh git plugin
alias ga="git add"
alias gd="git diff"
alias gp="git push"
alias gst="git status"
alias gco="git checkout"
alias gcmsg="git commit -m"
alias gb="git branch"
alias grb="git rebase"
